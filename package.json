{
    "name": "semrush-community-connector",
    "version": "0.0.1",
    "description": "Community connector for Semrush",
    "scripts": {
        "push": "cd src; clasp push; cd -",
        "test": "jest --watch --forceExit --detectOpenHandles --coverage  --coverageDirectory=coverage --testPathPattern=\"(/src/.*)\\.(test|spec).js$\"",
        "test:unit": "jest --watch --forceExit --detectOpenHandles --testPathPattern=\"(/src/.*)\\.(spec).js$\""
    },
    "repository": "https://github.com/jinified/semrush-api-connector",
    "author": "jinified <jinified@gmail.com>",
    "license": "MIT",
    "private": null,
    "dependencies": {
        "@google/clasp": "^2.1.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.4.4",
        "babel-core": "^6.26.3",
        "babel-jest": "^24.8.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^5.16.0",
        "eslint-plugin-jest": "^22.5.1",
        "jest": "^24.8.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*{js,jsx}"
        ],
        "coveragePathIgnorePatterns": [
            "node_modules",
            "src/mocks"
        ],
        "coverageReporters": [
            "json",
            "html",
            "lcov",
            "text",
            "clover"
        ]
    }
}
